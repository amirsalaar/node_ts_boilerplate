{
  "name": "node_ts_jest_eslint_boiler",
  "version": "1.0.0",
  "description": "",
  "main": "indexpress.js",
  "scripts": {
    "build": "npx tsc && npx copyfiles -u 1 -e \"src/**/*.ts\" \"src/**/*.*\" dist/",
    "watch": "tsc --watch -p ./",
    "debug": "npx cross-env NODE_ENV=development nodemon --nolazy ./dist/server.js",
    "docker-debug": "npm run build && docker-compose up",
    "lint": "eslint --ext .ts .",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write --loglevel silent './**/*.{json,js,ts,md}'",
    "test": "npx tsc && jest",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "start-dev": "npm run format && npm run lint && npx cross-env NODE_ENV=development nodemon --quiet --watch ./dist/server.js",
    "prepare": "husky install",
    "document": "npx typedoc"
  },
  "lint-staged": {
    "./**/*.{js,json,ts}": "eslint --cache --fix && npm run format"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.25.0",
    "cross-env": "^7.0.3",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "express-winston": "^4.2.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.7",
    "swagger-ui-express": "^4.3.0",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.33",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^12.2.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.5.1",
    "typedoc": "^0.22.11",
    "typescript": "^4.2.3"
  }
}
